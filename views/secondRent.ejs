            <%- include('partials/registerModal.ejs') %>
        <!-- model for register end -->

      

    <script>
                document.getElementById('rentForm').addEventListener('submit', function(event) {
            const propertyPhotos = document.getElementById('propertyPhotos');
            if (propertyPhotos.files.length < 3) {
                event.preventDefault();
                document.getElementById('error-message').style.display = 'block';
            } else {
                document.getElementById('error-message').style.display = 'none';
            }
        });
    </script>
  

    <script src="Js/main.js"></script>
