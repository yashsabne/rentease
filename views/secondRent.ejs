  <main>
    <div class="div-specification"> 
      <h3 id="statement">  </h3>
    </div><hr>
<div class="rent-main-section d-flex">


  <div class="rent-statement">
    <small>to make your home visible for renting or selling to all you need to fill the below details  </small>
  </div>

  <div class="additional-details" style="display: none;">
    <div class="form-group">
      <input type="text" class="form-control" id="newField1" name="newField1" placeholder="New Field 1" required>
    </div>
    <div class="form-group">
      <input type="text" class="form-control" id="newField2" name="newField2" placeholder="New Field 2" required>
    </div>
  </div>
  
  

  <div class="form-rent-home">
    <!-- <form action="/rent-sell-home" method="post">

        
 

<div class="submit-btn d-flex justify-content-center mt-3"> 
    <button type="submit" class="btn btn-success">Submit</button>
  </div>
    </form> -->


    <form action="/rent-sell-home-secondePage" method="post">
        <div class="input-group">
            <label for="propertyAddress">Property Address</label>
            <input type="text" id="propertyAddress" name="propertyAddress" placeholder="Enter the property address" required>
        </div>
        
        <div class="price-property d-flex"> 
        <div class="input-group mr-2">
            <label for="minPrice">Minimum Price</label>
            <input type="number" id="minPrice" name="minPrice" placeholder="Minimum Price" required>
        </div>
 
        <div class="input-group">
            <label for="maxPrice">Maximum Price</label>
            <input type="number" id="maxPrice" name="maxPrice" placeholder="Maximum Price" required>
        </div>
    </div>
        <div class="input-group">
            <label for="propertyType">Property Type</label>
            <select id="propertyType" name="propertyType" required>
                <option value="">Select type</option>
                <option value="apartment">Apartment</option>
                <option value="rooms">Rooms</option>
                <option value="house">House</option>
                <option value="villa">Villa</option>
                <option value="commercial">Commercial</option>
            </select>
        </div>
        <div class="input-group" style="display: flex;flex-direction: column;">
            <label for="propertyDescription">Property Description</label> 
            <textarea id="propertyDescription" name="propertyDescription" rows="4" placeholder="Describe the property" required></textarea>
        </div>
        <div class="input-group">
            <label for="propertySize">Property Size (in sq ft)</label>
            <input type="number" id="propertySize" name="propertySize" placeholder="Enter the property size" required>
        </div>
      
        <div class="submit-btn">
            <button type="submit">Submit</button>
        </div>
    </form>

  </div>
 
</div>
<%- include('partials/footer.ejs') %>
  </main>
    </div>

 
        <!-- model for login start -->
        <%- include('partials/loginModal.ejs') %>
