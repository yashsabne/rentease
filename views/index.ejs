      <%- include('partials/nav.ejs') %> <!--including navbar-->
        <main>
          <div class="div-specification">
            <h3 id="statement"> </h3>
          </div>
          <hr>
          <div id="carouselExampleControls" class="carousel slide d-flex justify-content-center" data-ride="carousel">
            <div class="carousel-inner" style=" width: 50vw;border: 1px solid black;">
              <div class="carousel-item active">
                <img class="d-block w-100" src="imgs/buyimg.jpg" alt="First slide">
              </div>
              <div class="carousel-item">
                <img class="d-block w-100" src="imgs/homeimg.jpg" alt="Second slide">
